c
c    Storage for sub-grid scale stress variables
c
      real ediff(lx1,ly1,lz1,lelv),
     $       dg2(lx1,ly1,lz1,lelv)

      common /cdsmag/ ediff, dg2

      real sij(lx1*ly1*lz1,ldim,ldim),
     $     snrm(lx1*ly1*lz1,lelv),
     $     Cs(lx1*ly1*lz1,lelv)
      common /dynsmg/ sij,snrm,Cs

